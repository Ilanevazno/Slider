{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": false,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": true,
        "noImplicitThis": false,
        "noImplicitReturns": false,
    },
}