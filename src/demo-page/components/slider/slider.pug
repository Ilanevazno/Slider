mixin slider (data)
  -
    const {
      minValue = 0,
      maxValue = 100,
      stepSize = 10,
      minValueCurrent = 0,
      maxValueCurrent = 0,
      axis = 'X',
      valueType = 'singleValue',
      withTooltip = true,
      withLabels = true,
    } = data;
  section.slider.js-slider(
    data-minValue= minValue
    data-maxValue= maxValue
    data-stepSize= stepSize
    data-minValueCurrent = minValueCurrent
    data-maxValueCurrent = maxValueCurrent
    data-axis= axis
    data-valueType= valueType
    data-withTooltip= withTooltip
    data-withLabels= withLabels
  )